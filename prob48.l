(defun pow(x n)
  (cond ((eq n 0) 1) (t (* (pow x (1- n)) x))))
(setq lt nil)
(defun proc(x)
  (cond ((null (eq x 1001))
	 (push (pow x x) lt) (proc (1+ x)))))
